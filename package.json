{
  "name": "trivia-art",
  "version": "0.0.1",
  "description": "Trivia Art",
  "main": "build/app/main.js",
  "scripts": {
    "start": "node build/app/main.js",
    "prebuild": "rimraf build cache/ts.build",
    "build": "tsc -p tsconfig.json --diagnostics",
    "build:watch": "npm run build -- --watch",
    "db:create": "node .bin/exec ./database/migrations sequelize db:create",
    "db:update": "npm run db:migrate && npm run db:seed",
    "db:migrate": "node .bin/exec ./database/migrations sequelize db:migrate",
    "db:seed": "node .bin/exec ./database/migrations sequelize db:seed:all",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --max-warnings 0",
    "lint:fix": "npm run lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petarzarkov/trivia-art.git"
  },
  "keywords": [
    "trivia",
    "api",
    "questions"
  ],
  "author": "Petar Zarkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petarzarkov/trivia-art/issues"
  },
  "homepage": "https://github.com/petarzarkov/trivia-art#readme",
  "dependencies": {
    "fastify": "^3.27.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "jest": "^27.3.0",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
